<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.mycompany.Tget_mini_web.dao.mybatis.CartDetail">
	
	<!-- 회원정보 얻기 -->
	<select id="selectByMid" parameterType="String" resultType="MemberDto">
		select mid
		from member
		where mid = #{mid}
	</select>
	
	<!-- 장바구니(카트 디테일)에 상품 집어넣기 -->
	<insert id="insertCartDetail" parameterType="CartDetailDto">
		insert into cartdetail (mid, pno)
		values (#{mid}, #{pno})
	</insert>
	
</mapper>